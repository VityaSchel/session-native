{
  "name": "backend",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun --bun src/index.ts",
    "dev": "bun --watch --bun src/index.ts",
    "compile": "bun compile:darwin:arm && bun compile:darwin:x64",
    "compile:darwin:arm": "bun build src/index.ts --compile --outfile=darwin_arm --target=bun-darwin-arm64 --minify",
    "compile:darwin:x64": "bun build src/index.ts --compile --outfile=darwin_x64 --target=bun-darwin-x64 --minify"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "eslint": "^9.8.0",
    "typescript-eslint": "^7.17.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@session.js/client": "0.0.45",
    "@session.js/file-keyval-storage": "^1.0.2",
    "@session.js/keypair": "^1.0.2",
    "@session.js/mnemonic": "^1.0.4",
    "@session.js/ons": "^1.0.3",
    "zod": "^3.23.8"
  }
}